// Define a variable 'i' with value 10
i = 10;

// Print "Hello, world!" using the 'Write' function
Write("Hello, world!");

// Define a variable 'x' with value 5
x = 5;

// Define a variable 'y' with value 7
y = 7;

// Print the value of the sum of 'x' and 'y'
z = x + y;
Write("The sum of x and y is " + z + ".");

// Use a for loop to print the values from 1 to 'i'
Write("Printing values from 1 to " + i + ":");
for (j = 1; j <= i; j = j + 1) {
    Write("Ok:  " + j + ".");
}


